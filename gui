import photo_file_rc

class Parametersandfunction(QtWidgets.QMainWindow, Ui_MainWindow):
    def __init__(self):
        super().__init__()
        self.ui = Ui_MainWindow()
        self.ui.setupUi(self)

        # 连接 "Run" 按钮的点击事件
        self.ui.b1.clicked.connect(self.run_class_StockV)
        self.ui.b2.clicked.connect(self.run_class_StockE)
        self.ui.b3.clicked.connect(self.run_class_StockM)

        self.graphicsView = self.ui.graphicsView
        self.scene = QtWidgets.QGraphicsScene()
        self.graphicsView.setScene(self.scene)
        self.resultLabel = self.ui.resultLabel

        self.input1_value = None
        self.input2_value = None
        self.stocks = None

        self.stockV_instance = None
        self.stockE_instance = None
        self.stockM_instance = None
    def show_image(self,image):
        pixmap = QtGui.QPixmap.fromImage(image)
        pixmap_item = QtWidgets.QGraphicsPixmapItem(pixmap)
        self.scene.clear()
        self.scene.addItem(pixmap_item)

    def generate_and_show_image(self):
        stockV_instance = StockData()
        image = stockV_instance.candle_plot()
        self.show_image(image)

    def run_class_StockV(self):
        self.input1_value = self.input1.text()
        self.input2_value = self.input2.text()
        self.stocks = self.input3.text()
        self.start = datetime.strptime(self.input1_value, "%Y-%m-%d").date()
        self.end = datetime.strptime(self.input2_value, "%Y-%m-%d").date()

        StockV_instance = StockData()
        StockV_instance.run()
        self.shared_parameters = {
            'input1_value': self.input1_value,
            'input2_value': self.input2_value,
            'stocks': self.stocks,
            'start': self.start,
            'end': self.end,
            'stockV_instance': StockV_instance
        }

        image = StockV_instance.candle_plot()
        self.show_image(image)

    def run_class_StockE(self):
        if self.shared_parameters is None:
            return
        StockM_instance = StockPrediction()
        StockM_instance.run()
        StockE_instance = StockEvaluation(model=StockM_instance)
        result_text = StockE_instance.graph()
        self.ui.resultLabel.setText(result_text)

    def run_class_StockM(self):
        if self.shared_parameters is None:
            return
        StockM_instance = StockPrediction()
        result_text = StockM_instance.run()
        self.ui.resultLabel.setText(result_text)
if __name__ == "__main__":
    import sys
    app = QtWidgets.QApplication(sys.argv)
    MainWindow = QtWidgets.QMainWindow()
    ui = Ui_MainWindow()
    ui.setupUi(MainWindow)
    MainWindow.show()
    sys.exit(app.exec_())
